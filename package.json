  "scripts": {
    "analyze": "webpack-bundle-analyzer .next/stats.json",
    "build": "next build",
    "build-images": "svgo -f images/raw -o images --config=.svgo && svgo -f images/raw-static -o static --config=.svgo",
    "dev": "concurrently -k -p name -n SERVER,API -c blue,cyan \"yarn dev-server\" \"yarn dev-api-server\"",
    "dev-api-server": "nodemon api-server --watch api-server --watch utils",
    "dev-server": "nodemon server.js --watch server.js",
    "start": "concurrently -k -p name -n SERVER,API -c blue,cyan \"yarn start-server\" \"yarn start-api-server\"",
    "start-api-server": "node api-server",
    "start-server": "node server.js"
  }
}
