    "classnames": "^2.2.5",
    "codemirror": "^5.36.0",
    "common-tags": "^1.7.2",
    "commonmark": "^0.28.1",
    "commonmark-react-object-renderer": "^1.0.0",
    "concurrently": "^3.5.1",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.23.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "next": "^5.0.0",
    "nodemon": "^1.17.2",
    "prop-types": "^15.6.1",
